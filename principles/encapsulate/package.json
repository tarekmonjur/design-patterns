    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "eslint": "^8.47.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.2",
        "pretty-quick": "^3.0.2",
        "rimraf": "^5.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
